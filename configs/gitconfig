[user]
    name = Dylan Griffith
    email = dgriffith@groupon.com
[core]
    editor = /usr/local/bin/vim
    excludesfile = ~/.gitignore_global
[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
    ui = auto
[alias]
    st = status
    ci = commit
    co = checkout
    br = branch
    df = diff
    dc = diff --cached
    fall = fetch --all
    cim = commit -m
    tree = log --graph --oneline --decorate --color --all
    tr = log --graph --oneline --decorate --color
    ln = log --pretty=format:'%Cblue%h %Cred* %C(yellow)%s'
[push]
    default = current
[credential]
    helper = osxkeychain
